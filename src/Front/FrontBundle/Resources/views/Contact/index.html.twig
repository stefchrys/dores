{% extends '::base.html.twig' %}
{% block slider %}
{% endblock %}
{% block lettre %}
{% endblock %}
{% block contenu %}
{%block agenda %}
{%endblock %}
<!--Contenu principal @media>1200px-->
<div class="row">
<div class="col-md-2"> 
</div>
<div class="col-md-8">           
<form action="{{ path('front_contact') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}
	<div class="form-group"> 
        {{ form_row(form.name) }}
	</div>
	<div class="form-group"> 
        {{ form_row(form.email) }}
	</div>
	<div class="form-group"> 
        {{ form_row(form.subject) }}
	</div>
	<div class="form-group"> 
        {{ form_row(form.body) }}
	</div>
	
	<div class="form-group"> 
        {{ form_rest(form) }}
	</div>
	<div class="form-group"> 
    <button type="submit" class="btn btn-info">Envoyer</button> 
	</div>
</form>
</div>
<div class="col-md-2"> 
</div>
</div>
 <!--fin contenu principal @media>1200px-->

{% endblock %}
        